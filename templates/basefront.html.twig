{% block styles %}
    <style>
        .sponsor-list {
            padding: 50px 0;
            background: #f8f8f8;
        }

        .container {
            width: 80%;
            margin: auto;
        }

        .section-title {
            text-align: center;
            font-size: 32px;
            margin-bottom: 30px;
        }

        .sponsor-card {
            display: flex;
            align-items: center;
            justify-content: space-between;
            background: #fff;
            padding: 20px;
            margin-bottom: 20px;
            border-radius: 8px;
            box-shadow: 0px 2px 10px rgba(0, 0, 0, 0.1);
        }

        .sponsor-date {
            text-align: center;
            font-size: 20px;
            font-weight: bold;
            color: #9c815e;
        }

        .sponsor-info {
            display: flex;
            align-items: center;
            flex-grow: 1;
            margin-left: 20px;
        }

        .sponsor-logo img {
            width: 80px;
            height: 80px;
            border-radius: 50%;
            object-fit: cover;
        }

        .sponsor-details {
            margin-left: 15px;
        }

        .sponsor-details h3 {
            font-size: 22px;
            margin-bottom: 5px;
        }

        .sponsor-action .btn {
            display: inline-block;
            padding: 10px 20px;
            border: 1px solid #000;
            text-decoration: none;
            color: #000;
            font-weight: bold;
            transition: all 0.3s;
        }

        .sponsor-action .btn:hover {
            background: #9c815e;
            color: #fff;
        }

       
@keyframes float {
    0% { transform: translateY(0px); }
    50% { transform: translateY(-5px); }
    100% { transform: translateY(0px); }
}

.sponsor-logo {
    animation: float 3s ease-in-out infinite;
}

.sponsor-logo {
    transition: transform 0.3s ease-in-out, box-shadow 0.3s ease-in-out;
}

.sponsor-logo:hover {
    transform: scale(1.1); /* Zoom de 10% */
    box-shadow: 5px 5px 15px rgba(0, 0, 0, 0.2); /* Augmentation de l'ombre */
}

    </style>
