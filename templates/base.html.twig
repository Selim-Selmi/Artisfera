<style>
   .carousel-control-prev, .carousel-control-next {
    background-color: green; /* Fond vert complet */
    width: 8%; /* Ajuste la taille du bouton */
    opacity: 1; /* Assure que le fond est bien visible */
    border-radius: 5px; /* Arrondi léger */
}

.carousel-control-prev:hover, .carousel-control-next:hover {
    background-color: darkgreen; /* Assombrit au survol */
}

.exhibition-two {
    padding: 60px 0;
    background-color: #f8f8f8;
}

.exhibition-two__single {
    border-radius: 10px;
    overflow: hidden;
    background: #fff;
    box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);
    transition: transform 0.3s ease;
}

.exhibition-two__single:hover {
    transform: translateY(-5px);
}

.exhibition-two__image {
    width: 100%;
    height: 250px; /* Fixe une hauteur uniforme pour toutes les images */
    overflow: hidden;
    display: flex;
    align-items: center;
    justify-content: center;
}

.exhibition-two__image img {
    width: 100%;
    height: 100%;
    object-fit: cover; /* Empêche la déformation des images */
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
}

.exhibition-two__content {
    padding: 15px;
    text-align: center;
}

.exhibition-two__content h3 {
    font-size: 20px;
    margin-bottom: 5px;
}

.exhibition-two__content span {
    display: block;
    font-size: 14px;
    color: #666;
    margin-bottom: 10px;
}

.exhibition-two__content p {
    font-size: 16px;
    color: #333;
}

.exhibition-two__link {
    display: inline-block;
    margin-top: 10px;
    padding: 8px 15px;
    border-radius: 5px;
    background: #ff5a5f;
    color: white;
    text-decoration: none;
    font-weight: bold;
    transition: background 0.3s ease;
}

.exhibition-two__link:hover {
    background: #e0484e;
}


</style>
